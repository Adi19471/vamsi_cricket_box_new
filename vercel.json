{
  "version": 2,
  "rootDirectory": "vamsi_cricket_box_new",
  "buildCommand": "python manage.py migrate && python manage.py collectstatic --noinput",
  "builds": [
    {
      "src": "cricket_project/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "cricket_project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "cricket_project.settings",
    "DEBUG": "False"
  }
}
